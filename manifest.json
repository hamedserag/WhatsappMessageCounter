{
  "name": "Minders Msg Counter",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/content.js","js/first-entry.js","js/popup.js","js/jquery.js"],
    "css":["css/style.css"]
  }],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "MMC"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ]
}
